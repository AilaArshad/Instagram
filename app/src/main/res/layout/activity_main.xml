<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/main"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:background="@color/white"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/insta"
        android:layout_width="160dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/aguafina_script"
        android:paddingStart="8dp"
        android:paddingEnd="3dp"
        android:layout_marginBottom="15dp"
        android:text="@string/insta"
        android:textColor="@color/black"
        android:textSize="40sp"
        android:textStyle="bold" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:drawableStart="@drawable/messenger"
        android:layout_alignParentEnd="true"
        android:paddingEnd="9dp"
        android:layout_marginTop="7dp"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:drawableStart="@drawable/heart"
        android:layout_alignParentEnd="true"
        android:paddingEnd="60dp"
        android:layout_marginTop="5dp"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:drawableStart="@drawable/more"
        android:layout_alignParentEnd="true"
        android:paddingEnd="120dp"
        android:layout_marginTop="7dp"/>

    <androidx.cardview.widget.CardView
        android:id="@+id/mine"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:cardCornerRadius="100dp"
        android:layout_below="@id/insta"
        android:layout_marginLeft="20dp">
        <ImageView
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:scaleType="centerCrop"
            android:src="@drawable/mine"/>
    </androidx.cardview.widget.CardView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/story"
        android:fontFamily="sans-serif-black"
        android:textColor="@color/black"
        android:textSize="10sp"
        android:layout_below="@id/mine"
        android:layout_marginStart="30dp"
        android:layout_marginTop="10dp"/>

    <androidx.cardview.widget.CardView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/insta"
        app:cardCornerRadius="50dp"
        android:layout_marginLeft="108dp">
        <com.google.android.material.imageview.ShapeableImageView
            android:layout_width="78dp"
            android:layout_height="78dp"
            android:src="@drawable/stories_border"
            android:scaleType="fitCenter"
            app:shapeAppearanceOverlay="@style/Circular"
            />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/jisoo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:cardCornerRadius="100dp"
        android:layout_below="@id/insta"
        android:layout_marginLeft="112dp"
        android:layout_marginTop="4dp">
        <com.google.android.material.imageview.ShapeableImageView
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:scaleType="centerCrop"
            android:src="@drawable/jisoo"
            app:shapeAppearanceOverlay="@style/Circular"/>
    </androidx.cardview.widget.CardView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/jiso"
        android:fontFamily="sans-serif-black"
        android:textColor="@color/black"
        android:textSize="10sp"
        android:layout_below="@id/jisoo"
        android:layout_marginLeft="125dp"
        android:layout_marginTop="7dp"/>

    <androidx.cardview.widget.CardView
        android:id="@+id/enuwoo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/insta"
        app:cardCornerRadius="50dp"
        android:layout_marginLeft="195.5dp">
        <com.google.android.material.imageview.ShapeableImageView
            android:layout_width="78dp"
            android:layout_height="78dp"
            android:src="@drawable/stories_border"
            android:scaleType="fitCenter"
            app:shapeAppearanceOverlay="@style/Circular"
            />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:cardCornerRadius="100dp"
        android:layout_below="@id/insta"
        android:layout_marginLeft="200dp"
        android:layout_marginTop="4dp">
        <ImageView
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:scaleType="centerCrop"
            android:src="@drawable/eunwoo"/>
    </androidx.cardview.widget.CardView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/eun"
        android:fontFamily="sans-serif-black"
        android:textColor="@color/black"
        android:textSize="10sp"
        android:layout_below="@id/enuwoo"
        android:layout_marginLeft="215dp"
        android:layout_marginTop="3dp"/>

    <androidx.cardview.widget.CardView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/insta"
        app:cardCornerRadius="50dp"
        android:layout_marginLeft="285dp">
        <com.google.android.material.imageview.ShapeableImageView
            android:layout_width="78dp"
            android:layout_height="78dp"
            android:src="@drawable/stories_border"
            android:scaleType="fitCenter"
            app:shapeAppearanceOverlay="@style/Circular"
            />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/moon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:cardCornerRadius="100dp"
        android:layout_below="@id/insta"
        android:layout_marginLeft="289dp"
        android:layout_marginTop="4.5dp">
        <ImageView
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:scaleType="centerCrop"
            android:src="@drawable/moon"
            />
    </androidx.cardview.widget.CardView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/mon"
        android:fontFamily="sans-serif-black"
        android:textColor="@color/black"
        android:textSize="10sp"
        android:layout_below="@id/moon"
        android:layout_marginLeft="300dp"
        android:layout_marginTop="7dp"/>

    <androidx.cardview.widget.CardView
        android:id="@+id/hwang"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:cardCornerRadius="100dp"
        android:layout_below="@id/insta"
        android:layout_marginLeft="375dp">
        <com.google.android.material.imageview.ShapeableImageView
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:scaleType="centerCrop"
            android:src="@drawable/hwang"
            />
    </androidx.cardview.widget.CardView>

    <ImageView
        android:id="@+id/post"
        android:layout_width="match_parent"
        android:layout_height="400dp"
        android:src="@drawable/post"
        android:scaleType="fitXY"
        android:layout_marginTop="250dp"
        />

    <androidx.cardview.widget.CardView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/mine"
        app:cardCornerRadius="50dp"
        android:layout_marginTop="55dp"
        android:layout_marginStart="6dp">
        <com.google.android.material.imageview.ShapeableImageView
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:src="@drawable/stories_border"
            android:scaleType="fitCenter"
            app:shapeAppearanceOverlay="@style/Circular"
            />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/moon_post"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:cardCornerRadius="100dp"
        android:layout_below="@id/mine"
        android:layout_marginTop="57.5dp"
        android:layout_marginStart="8.5dp">
        <ImageView
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:scaleType="centerCrop"
            android:src="@drawable/moon"
            />
    </androidx.cardview.widget.CardView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/mon"
        android:fontFamily="sans-serif-black"
        android:textColor="@color/black"
        android:textSize="13sp"
        android:layout_toRightOf="@+id/moon_post"
        android:layout_marginTop="210dp"
        android:layout_marginStart="10dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:drawableStart="@drawable/dots"
        android:layout_below="@id/hwang"
        android:layout_marginTop="65dp"
        android:layout_marginLeft="360dp"/>

    <TextView
        android:id="@+id/heart"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:drawableStart="@drawable/heart"
        android:layout_below="@id/post"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="25dp"/>

    <androidx.cardview.widget.CardView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/heart"
        app:cardCornerRadius="50dp"
        android:layout_marginStart="6dp"
        android:layout_marginTop="30dp">
        <com.google.android.material.imageview.ShapeableImageView
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:src="@drawable/stories_border"
            android:scaleType="fitCenter"
            app:shapeAppearanceOverlay="@style/Circular"
            />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:cardCornerRadius="100dp"
        android:layout_below="@id/heart"
        android:layout_marginStart="8.5dp"
        android:layout_marginTop="33dp">
        <ImageView
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:scaleType="centerCrop"
            android:src="@drawable/eunwoo"/>
    </androidx.cardview.widget.CardView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/eun"
        android:fontFamily="sans-serif-black"
        android:textColor="@color/black"
        android:textSize="13sp"
        android:layout_below="@id/heart"
        android:layout_marginStart="55dp"
        android:layout_marginTop="40dp"/>

    <TextView
        android:id="@+id/option"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:drawableStart="@drawable/dots"
        android:layout_below="@id/save"
        android:layout_marginTop="33dp"
        android:layout_marginLeft="360dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:drawableStart="@drawable/comment"
        android:layout_below="@id/post"
        android:layout_marginLeft="60dp"
        android:layout_marginTop="29dp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:drawableStart="@drawable/sent"
        android:layout_below="@id/post"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="105dp"/>
    <TextView
        android:id="@+id/save"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:drawableStart="@drawable/save"
        android:layout_below="@id/post"
        android:layout_marginTop="22dp"
        android:layout_alignParentEnd="true"
        android:layout_marginLeft="110dp"/>

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:scaleType="fitXY"
        android:src="@drawable/mountain"
        android:layout_below="@id/option"
        android:layout_marginTop="20dp"
        />

</RelativeLayout>